#: ../brita.py:38
#, python-format
msgid "Current brightness: %s / %s (%s%%)"
msgstr "Ustawiona jasność: %s / %s (%s%%)"

#: ../brita.py:43
msgid "This script works only in Linux environment."
msgstr "Ten skrypt działa tylko w systemie Linux."

#: ../brita.py:50
#, python-format
msgid "Found backlight device: %s"
msgstr "Znaleziono urządzenie podświetlenia ekranu: %s"

#: ../brita.py:54
msgid "Brightness settings aren't available for the device.\n"
msgstr "To urządzenie nie udostępnia ustawień jasności podświetlnia.\n"

#: ../brita.py:113
#, python-format
msgid "Brita %s - backlight adjuster for Linux"
msgstr "Brita %s - program zarządzający podświetleniem ekranu dla Linuksa"

#: ../brita.py:114
msgid ""
"usage: brita [--help] value\n"
"\n"
"\tvalue can be absolute (ie. 400),\n"
"\t\tin percents (ie. 50%)\n"
"\t\tor relative to the current value (ie. -500, +30%, -10%)\n"
"\t\n"
"\tprogram executed without arguments shows the current brightness"
msgstr ""
"użycie: brita [--help] wartość\n"
"\n"
"\tvwartość może być absolutna (np. 400),\n"
"\t\tpodana w procentach (np. 50%)\n"
"\t\tlub w relacji do obecnej wartości (np.500, +30%, -10%)\n"
"\t\n"
"\tprogram wywołany bez wartości pokazuje obecnie ustawioną jasność"
